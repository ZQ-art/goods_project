terData.password\"\r\n            label=\"密码\"\r\n            placeholder=\"请输入密码\"\r\n            type=\"password\"\r\n            @change=\"checkPasswordFunction\"\r\n          ></van-field>\r\n          <van-field\r\n            v-model=\"passwordVertify\"\r\n            label=\"密码验证\"\r\n            placeholder=\"再次输入密码\"\r\n            type=\"password\"\r\n            :error=\"checkPassword.flag\"\r\n            @change=\"checkPasswordFunction\"\r\n            :error-message=\"checkPassword.message\"\r\n          ></van-field>\r\n          <van-button type=\"primary\" @click=\"login\">登录</van-button>\r\n          <van-button type=\"info\" class=\"last\" @click=\"register\" :disabled=\"checkPassword.flag\">注册</van-button>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport config from \"@/config/config.json\";\r\n\r\nexport default {\r\n  name: \"auth\",\r\n  data() {\r\n    return {\r\n      loginData: {\r\n        userName: \"\",\r\n        password: \"\",\r\n      },\r\n      registerData: {\r\n        userName: \"\",\r\n        password: \"\",\r\n      },\r\n      passwordVertify: \"\",\r\n      loginPage: true,\r\n      checkPassword: {\r\n        flag: false,\r\n        message: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      if (this.loginPage == false) {\r\n        this.loginPage = true;\r\n      } else {\r\n        if (this.checkValue(this.loginData)) {\r\n          this.axios\r\n            .post(config.severAddress + \"/signIn\", this.loginData)\r\n            .then((res) => {\r\n              if (res.data.status) {\r\n                console.log(\"登录成功！\");\r\n              } else {\r\n                console.log(res.data.msg);\r\n                this.$notify({\r\n                  type: \"danger\",\r\n                  message: res.data.msg,\r\n                  duration: \"750\",\r\n                });\r\n              }\r\n            });\r\n        } else {\r\n 