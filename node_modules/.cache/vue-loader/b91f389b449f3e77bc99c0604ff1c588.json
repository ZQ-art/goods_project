n          name=\"代取物名称\"\r\n          label=\"代取物名称\"\r\n          placeholder=\"请填写您的代取物名称\"\r\n          :rules=\"[{ required: true, message: '代取物名称' }]\"\r\n        />\r\n        <van-field\r\n          v-model=\"data.time\"\r\n          readonly\r\n          name=\"限定时间\"\r\n          label=\"限定时间\"\r\n          v-if=\"showTime\"\r\n          @click=\"showTime = false\"\r\n          placeholder=\"请选择限定时间\"\r\n        />\r\n        <van-datetime-picker\r\n          v-model=\"data.timeTmp\"\r\n          type=\"time\"\r\n          title=\"选择限定时间\"\r\n          :min-hour=\"10\"\r\n          :max-hour=\"20\"\r\n          v-else\r\n          @confirm=\"timeYes\"\r\n          @cancel=\"showTime = true\"\r\n        />\r\n        <van-field name=\"stepper\" label=\"悬赏金额(元)\">\r\n          <template #input>\r\n            <van-stepper v-model=\"data.money\" step=\"0.5\" />\r\n          </template>\r\n        </van-field>\r\n        <van-field\r\n          v-model=\"data.msg\"\r\n          rows=\"2\"\r\n          autosize\r\n          label=\"留言\"\r\n          type=\"textarea\"\r\n          maxlength=\"50\"\r\n          placeholder=\"请输入留言\"\r\n          show-word-limit\r\n        />\r\n      </van-form>\r\n      <div style=\"margin: 16px\" class=\"actions\">\r\n        <van-button type=\"primary\" round @click=\"active--\">上一步</van-button>\r\n        <van-button round type=\"info\" native-type=\"submit\" @click=\"submit\"\r\n          >提交</van-button\r\n        >\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"third\" v-else-if=\"active === 2\">等待接单</div>\r\n\r\n    <div class=\"four\" v-else>完成交易</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"offerReward\",\r\n  data() {\r\n    return {\r\n      checked: true,\r\n      active: 0,\r\n      show: false,\r\n      showPicker: false,\r\n      showTime: true,\r\n      timeLimit: \"\",\r\n      columns: [\r\n        // 第一列\r\n        {\r\n          values: [\"极重\", \"重\", \"中\", \"轻\", \"极轻\"],\r\n          defaultIndex: 2,\r\n        },\r\n        // 第二列\r\n        {\r\n          values: [\"极大\", \"大\", \"中\", \"小\", \"极小\"],\r\n          defaultIndex: 1,\r\n        },\r\n      ],\r\n      actions: [\r\n        { name: \"南区快递点\" },\r\n        { name: \"北区快递点\" },\r\n        { name: \"29号楼外卖点\" },\r\n        { name: \"21号楼外卖点\" },\r\n      ],\r\n      search: \"\",\r\n      data: {\r\n        size: [],\r\n        name: \"\",\r\n        time: \"\",\r\n        timeTmp: \"\",\r\n        money: 0,\r\n        msg: \"\",\r\n        place: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    onConfirm(value) {\r\n      this.data.size = value;\r\n      this.showPicker = false;\r\n    },\r\n    onSubmit(values) {\r\n      console.log(\"submit\", values);\r\n    },\r\n    submit() {\r\n      console.log(\"提交悬赏\");\r\n      console.log(this.data);\r\n      this.active++;\r\n    },\r\n    timeYes() {\r\n      this.data.time = this.data.timeTmp;\r\n      this.showTime = true;\r\n    },\r\n    placeSelect(action) {\r\n      this.data.place = action;\r\n    },\r\n  },\r\n  computed: {\r\n    showSize() {\r\n      return this.data.size[0] === undefined\r\n        ? \"\"\r\n        : this.data.size[0] + \",\" + this.data.size[1];\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#offerReward {\r\n  .first {\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-top: 20px;\r\n  }\r\n  .second {\r\n    .actions {\r\n      display: flex;\r\n      justify-content: space-around;\r\n      .van-button {\r\n        flex: 0.4;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]}]}